<%include partials/header%>
    <div class="page-header header-filter" data-parallax="true" style="background-image: url(https://www.scienceabc.com/wp-content/uploads/2016/08/Passenger-Airliner-flying-in-the-clouds-airplaneAndrey-Armyagovs.jpg); transform: translate3d(0px, 0px, 0px);">
        <div class="container">
            
    <div class="row">
        <div class="col s12 m12">
          <div class="card blue-grey darken-1">
            <form action="#" method="post">

                <div class="card-content white-text" style="padding-bottom: 10px;">

                    <span class="card-title white-text">Enter Report Information</span>

					<input type="hidden" name="userID" value="<%= id %>" />

                    <div class="input-field">
                        <input name="reportName" type="text">
                        <label for="reportName">Report Name</label>
                    </div>

                    <div class="input-field">
                        <input name="doc_link" type="text">
                        <label for="doc_link">Document Link</label>
                    </div>

                    <div class="input-field">
                        <input name="lab_name" type="text">
                        <label for="lab_name">Lab Name</label>
                    </div>

                    <div class="input-field">
                        <input name="location" type="text">
                        <label for="location">Lab Location</label>
                    </div>

                </div>

                <button class="col s12 btn waves-effect waves-light" type="submit">Submit
                    <i class="material-icons right">send</i>
                </button>

              </form>

          </div>
        </div>
    </div>
    
        </div>
    </div>

	<!--   Core JS Files   -->
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
	<script src="/js/bootstrap.min.js" type="text/javascript"></script>
	<script src="/js/material.min.js"></script>
	<!--    Control Center for Material Kit: activating the ripples, parallax effects-->
	<script src="/js/material-kit.min.js" type="text/javascript"></script>

    <script type="text/javascript" src="/js/materialize.min.js"></script>

	<script type="text/javascript">
