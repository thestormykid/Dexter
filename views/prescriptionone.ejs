
<%include partials/header%>
    <div class="page-header header-filter" data-parallax="true" style="background-image: url(https://www.scienceabc.com/wp-content/uploads/2016/08/Passenger-Airliner-flying-in-the-clouds-airplaneAndrey-Armyagovs.jpg); transform: translate3d(0px, 0px, 0px);">
        <div class="container">
            
    <div class="row">
        <div class="col s12 m12">
            
          <div style="padding-top: 10px;" class="card blue-grey darken-1">

            <span style="margin-left:10px;" class="card-title white-text text-darken-4">Select Prescription</span>
            
            <ul class="collection">
            	<%prescription.forEach(function(pres){ %>
            	<%if(pres._id==presid){%>


                <li class="collection-item avatar">
                <i class="material-icons circle">folder</i>
                <span class="title"><%=pres.doctorId%> : <%=pres.doctor_name%><br></span>
                <p><%=pres.disease%>  - <%=pres.createdAt%><br>
               <%=pres.hospital_name%>
                </p>
                <%-pres.prescriptionDetails%>
                </li>
                <%}%>
           <%})%>
           <a href="/prescription/<%=userid%>"><button>BACK</button></a>
            </ul>


            </div>
        </div>
      </div>
    
        </div>
    </div>

	<!--   Core JS Files   -->
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
	<script src="/js/bootstrap.min.js" type="text/javascript"></script>
	<script src="/js/material.min.js"></script>
	<!--    Control Center for Material Kit: activating the ripples, parallax effects-->
	<script src="/js/material-kit.min.js" type="text/javascript"></script>

    <script type="text/javascript" src="/js/materialize.min.js"></script>

	<script type="text/javascript">
		// $(document).ready(function(){
